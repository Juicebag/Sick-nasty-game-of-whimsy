class Enemy:
    def __init__(self, Name, HP,Att, End, Speed, ):
        self.Name = Name
        self.HP = HP
        self.Attack = Att
        self.End = End
        # Put the damage Calculator here + self.Damage = Damage
 
    def is_alive(self):
        return self.HP > 0

Enemy("Ogre", 40, 15, 10,5):
Enemy("Goblin",15,10,10,5):
Enemy("Raider",20,20,5,20):


    
