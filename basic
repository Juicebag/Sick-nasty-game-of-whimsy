Class Character(object):
	def __init__ (self, status, stats, inv, pos, helm, chest, leg, gaunt, onhand, offhand)
		
	def __repr__(self)
		return (self)
	def move (self, dest)
		if dest in self.pos.adj:
			return (self.pos = dest)
		else:
			print ("you can't got to" dest "from here")
			return (self.pos = self.pos)
		
	
Class room(object)
	def __init__ (code, adj, desc, items, inhab)

def interpret(comm)
	if ' ' not in comm == 1 or( 'move' not in comm == 1 '
		return ('I don't understand that command')
class EnemyRoom(MapTile):
    def __init__(self, dest, enemy):
        self.enemy = enemy
        super().__init__(dest)
 
    def modify_player(self, Character):
        if self.enemy.is_alive():
            the_player.hp = the_player.hp - self.enemy.damage
          
